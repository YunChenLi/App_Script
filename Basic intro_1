function myFunction(){

var firstNum = SpreadsheetApp.getActiveSpreadsheet()
.getActiveSheet().getRange(3,3).getValue();

var secondNum = SpreadsheetApp.getActiveSpreadsheet()
.getActiveSheet().getRange(3,5).getValue();

var outcome = firstNum * secondNum;

SpreadsheetApp.getActiveSpreadsheet()
.getActiveSheet().getRange(3,4).setValue(outcome);


//console.log(firstNum);
//console.log(secondNum);
}


